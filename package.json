{
  "name": "rmrk-dao-consolidator",
  "version": "0.0.1",
  "description": "Custom RMRK consolidator for the RMRK::DAO project",
  "main": "index.js",
  "repository": "git@gitlab.com:rmrk-dao/rmrk-dao-consolidator.git",
  "author": "Spencer Brown <git@spencerwebservices.com>",
  "private": true,
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "dotenv -e .env node ./build/main.js",
    "dev": "dotenv -e .env ts-node ./src/main.ts",
    "consolidate": "dotenv -e .env ts-node ./src/rmrk2/consolidator.ts",
    "prisma:generate": "npx prisma generate",
    "postinstall": "yarn prisma:generate",
    "export": "ts-node ./src/rmrk2/export.ts"
  },
  "dependencies": {
    "@polkadot/api": "^8.10.1",
    "@prisma/client": "4.0.0",
    "@types/node": "^18.0.0",
    "JSONStream": "^1.3.5",
    "dotenv-cli": "^5.1.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "prisma": "^4.0.0",
    "rmrk-tools": "^2.0.85",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5"
  }
}

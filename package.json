{
  "name": "rmrk-dao-consolidator",
  "version": "1.0.6",
  "description": "Custom RMRK consolidator for the RMRK::DAO project",
  "main": "index.js",
  "repository": "git@gitlab.com:rmrk-dao/rmrk-dao-consolidator.git",
  "author": "Spencer Brown <git@spencerwebservices.com>",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:integration": "dotenv -e .env.integration.test jest -- --config jest.integration.config.js",
    "test:docker": "yarn test",
    "build": "tsc",
    "start": "dotenv -e .env node ./build/main.js",
    "dev": "dotenv -e .env ts-node ./src/main.ts",
    "consolidate": "dotenv -e .env ts-node ./src/cli/consolidate.ts",
    "prisma:generate": "npx prisma generate",
    "postinstall": "yarn prisma:generate",
    "export": "ts-node ./src/rmrk2/export.ts",
    "import-from-json": "ts-node ./src/rmrk2/import.ts",
    "sync-block-time": "ts-node ./src/cli/sync-block-time.ts",
    "example-seed": "dotenv -e .env ts-node ./src/cli/example-seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.171.0",
    "@polkadot/api": "^9.x",
    "@polkadot/keyring": "^10.x",
    "@polkadot/util": "^10.x",
    "@polkadot/util-crypto": "^10.x",
    "@prisma/client": "4.0.0",
    "@types/node": "^18.0.0",
    "JSONStream": "^1.3.5",
    "async-exit-hook": "^2.0.1",
    "dotenv-cli": "^5.1.0",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "prisma": "4.0.0",
    "rmrk-tools": "https://github.com/rmrkdao/rmrk-tools/releases/download/v3.0.2/rmrk-tools-3.0.2.tgz",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@ngneat/falso": "^6.1.0",
    "@types/async-exit-hook": "^2.0.0",
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.182",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5"
  }
}
